{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [],
    "baseUrl": "./",
    "paths": {
      "@env/*": [
        "environments/*"
      ],
      "@shared/*": [
        "app/shared/*"
      ],
      "@core/*": [
        "app/core/*"
      ],
      "@admin/*": [
        "app/admin/*"
      ],
      "@user/*": [
        "app/user/*"
      ],
      "@dev-console/*": [
        "app/dev-console/*"
      ],
      "@store/*": [
        "app/store/*"
      ],
      "@reducers": [
        "app/reducers"
      ]
    }
  },
  "exclude": [
    "../unit-tests/test.ts",
    "**/*.spec.ts",
    "**/assets/js/plugins/**"
  ]
}
