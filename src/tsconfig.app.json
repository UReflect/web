{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [],
    "paths": {
      "@env/*": [
        "envvironements/*"
      ],
      "@shared/*": [
        "shared/*"
      ],
      "@core/*": [
        "core/*"
      ],
      "@admin/*": [
        "admin/*"
      ],
      "@user/*": [
        "user/*"
      ],
      "@dev-console/*": [
        "dev-console/*"
      ],
      "@store/*": [
        "store/*"
      ]
    }
  },
  "exclude": [
    "../unit-tests/test.ts",
    "**/*.spec.ts",
    "**/assets/js/plugins/**"
  ]
}
